{
  "editor.formatOnPaste": true,
  "editor.tabSize": 2,
  "files.exclude": {
    "**/node_modules": true,
    "**/venv": true,
    "**/.next": true,
    "**/.pytest_cache": true,
    "**/__pycache__": true,
    "**/.ipynb_checkpoints": true,
  },
  "go.formatTool": "goimports",
  "go.testFlags": [
    "-v"
  ],
  "go.vetFlags": [
    "-structtag=false"
  ],
  "tslint.jsEnable": true,
  "go.inferGopath": false
}