{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": [
      false
    ],
    "semicolon": [
      true,
      "always"
    ],
    "object-literal-sort-keys": [
      false
    ],
    "member-ordering": [
      true,
      {
        "order": "fields-first"
      }
    ],
    "max-classes-per-file": false,
  },
  "rulesDirectory": []
}